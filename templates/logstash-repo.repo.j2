[logstash-{{ ls_major_version }}]
name=Logstash repository for {{ ls_major_version }} packages
baseurl=https://artifacts.elastic.co/packages/{{ ls_major_version }}/yum
gpgcheck=1
gpgkey=https://artifacts.elastic.co/GPG-KEY-elasticsearch
enabled=1
{% if es_proxy_host is defined and es_proxy_port is defined %}
proxy=http://{{ es_proxy_host }}:{{es_proxy_port}}
{% endif %}
autorefresh=1
type=rpm-md
